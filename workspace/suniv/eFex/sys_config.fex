[update_key]
;----------------------------------------------------------------------------------
;-------key_type  0:不需要强制升级   1:单个按键    2:两个按键组合             -------
;----------------------------------------------------------------------------------
key_type  = 1

;----------------------------------------------------------------------------------
;------------------   以下是只有单个按键(0:拉低  1:拉高)的情况   --------------------
;----------------------------------------------------------------------------------
key_value0  = 0
port0				= 1
port_num0   = 1 

;----------------------------------------------------------------------------------
;----------------------       以下是组合按键        ------------------------------
;----------------------------------------------------------------------------------
key_value1  = 0
port1				= 1
port_num1   = 1

key_value2  = 0
port2				= 1
port_num2   = 2

