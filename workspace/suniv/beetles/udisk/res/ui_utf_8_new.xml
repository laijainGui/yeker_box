			<device_type>curtain</device_type><!--窗帘类型-->
			<device_id>0x02</device_id> <!--设备ID-->
			<device_name>living room curtain</device_name> <!--设备名字，备用属性-->
			<relay> <!--控制该窗帘的继电器列表-->
				<relay_open>0x21</relay_open> <!--窗帘打开的继电器ID-->
				<relay_close>0x22</relay_close> <!--窗帘关闭的继电器ID-->
			</relay>
			<button> <!--控制该空调的button列表-->
				<button_open>0x0201</button_open> <!--窗帘打开的按键ID，对应于page中的itemid-->
				<button_close>0x0202</button_close> <!--窗帘关闭的按键ID-->
				<button_stop>0x0203</button_stop> <!--窗帘停止的按键ID-->
			</button>
		</device>
		
		<device>
			<device_type>arc</device_type><!--空调类型-->
			<device_id>0x03</device_id> <!--设备ID-->
			<device_name>living room curtain</device_name> <!--设备名字，备用属性-->
			<relay> <!--控制该空调的继电器列表-->
				<relay_speed_1>0x31</relay_speed_1> <!--1档的继电器ID-->
				<relay_speed_2>0x32</relay_speed_2> <!--2档的继电器ID-->
				<relay_speed_3>0x33</relay_speed_3> <!--3档的继电器ID-->
				<relay_cool>0x34</relay_cool> <!--制冷的继电器ID-->
				<relay_heat>0x35</relay_heat> <!--制热的继电器ID-->
			</relay>
			<button> <!--控制该空调的button列表-->
				<button_spped_1>0x0301</button_spped_1> <!--1档的按键ID，对应于page中的itemid-->
				<button_spped_2>0x0302</button_spped_2> <!--2档的按键ID-->
				<button_spped_3>0x0303</button_spped_3> <!--3档的按键ID-->
				<button_cool>0x0304</button_cool> <!--制冷的按键ID-->
				<button_heat>0x0305</button_heat> <!--制热的按键ID-->
				<button_add>0x0306</button_add> <!--温度加的按键ID-->
				<button_dec>0x0307</button_dec> <!--温度减的按键ID-->
				<button_power>0x0308</button_power> <!--电源开关的按键ID-->
			</button>
			<disp_set_temp>0x0309</disp_set_temp> <!--设置温度显示框的ID，对应于page中的itemid-->
			<disp_real_temp>0x0310</disp_real_temp> <!--实时温度显示框的ID，对应于page中的itemid-->
		</device>
		
		<!--添加场景（模式）-->
		<scene>
			<scene_name>work</scene_name> <!--场景名称-->
			<scene_id>0x01</scene_id> <!--场景ID-->
			<!--切换到该场景时需要执行的动作列表-->
			<scene_action> <!--动作1-->
				<device_id>0x01</device_id> <!--设备ID，对应于上面定义的设备ID-->
				<device_type>light</device_type> <!--设备类型-->
				<device_state>on</device_state> <!--执行该动作后设备要达到的状态，灯光状态on/off-->
			</scene_action>
			<scene_action> <!--动作2-->
				<device_id>0x02</device_id>
				<device_type>curtain</device_type>
				<device_state>open</device_state> <!--窗帘状态open/close-->
			</scene_action>
			<scene_action> <!--动作3-->
				<device_id>0x03</device_id>
				<device_type>arc</device_type>
				<device_state>heat</device_state> <!--空调状态cool/heat-->
				<fan_speed>2</fan_speed> <!--空调要调到第几档1/2/3-->
			</scene_action>
		</scene>
